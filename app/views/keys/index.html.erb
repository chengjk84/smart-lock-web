<div class='container'>
  <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
    <h1 class="display-4">Keys#index</h1>
    <p class="lead">Here is the list of ekeys belong to Lock ID <%= params[:lock_id] %>.</p>
  </div>
  
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">keyId</th>
        <th scope="col">keyStatus</th>
        <th scope="col">keyName</th>
        <th scope="col">startDate</th>
        <th scope="col">endDate</th>
        <th scope="col">keyRight</th>
        <th scope="col">remoteEnable</th>
        <th scope="col">username</th>
      </tr>
    </thead>
    <tbody>
    <% @keys["list"].each do |key| %>
      <tr>
        <td scope="row"><%= key["keyId"]%></td>
        <td><%= key["keyStatus"]%></td>
        <td><%= key["keyName"]%></td>
        <td><%= Time.at(key["startDate"]/1000) %></td>
        <td><%= Time.at(key["endDate"]/1000) %></td>
        <td><%= key["keyRight"]%></td>
        <td><%= key["remoteEnable"]%></td>
        <td><%= key["username"]%></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <%= link_to "Send eKeys", new_lock_key_path, class: "btn btn-secondary" %>
  
</div>