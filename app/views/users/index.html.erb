<div class='container'>
  <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
    <h1 class="display-4">Users#index</h1>
    <p class="lead">Find me in app/views/users/index.html.erb.</p>
  </div>
  
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">User ID</th>
        <th scope="col">Registered Time</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
    <% @users["list"].each do |user| %>
      <tr>
        <th scope="row"><%= user["userid"] %></th>
        <td><%= Time.at(user["regtime"]/1000) %></td>
        <td>Destroy</td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>